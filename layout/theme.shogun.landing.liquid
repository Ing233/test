        function fadeInPageMob(){
          if(!pageFadedIn) {
            document.body.classList.add("cc-animate-enabled");
            document.body.style.transition = 'opacity 0.4s';
            setTimeout(function () {
              document.body.style.opacity = 1;
            }, 200);
            pageFadedIn = true;
          }
        }

