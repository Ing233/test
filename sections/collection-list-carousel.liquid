{% assign has_description = false %}
{% for this_collection in section.settings.collection_list %}
    {% if this_collection.title == collection.title %}
        {% assign has_description = true %}
    {% endif %}
{% endfor %}

<div id="collection_list_carousel">
  {%- comment -%} 系列页封面 {%- endcomment -%}
    {% if has_description and section.settings.show_collection_image and collection.featured_image %}
      <div class="collection-header image-overlay image-overlay--bg-full needs-alt-logo">
        <div class="rimage-outer-wrapper rimage-background lazyload fade-in"
             data-cc-animate="cc-fade-in-zoom-out"
             data-bgset="{% render 'bgset', image: collection.featured_image %}"
             data-sizes="auto"
             data-parent-fit="cover">
          <noscript>
            <div class="rimage-wrapper" style="padding-top:{{ 1 | divided_by: collection.featured_image.aspect_ratio | times: 100 }}%">
              <img src="{{ collection.featured_image | img_url: '1024x1024' }}" alt="{{ collection.featured_image.alt | escape }}" class="rimage__image">
            </div>
          </noscript>
        </div>

        <div class="overlay-type overlay" data-cc-animate="cc-fade-in-zoom-out">
          <div class="overlay__content">
    {% endif %}

      <div class="central">
        <div class="content transparent">
          {% if section.settings.show_collection_content %}
            {% if collection.description != blank %}
              <h1 class="line-1 feature-header" data-cc-animate>{{ collection.title }}</h1>

              {% if section.settings.description_position == "top" %}
                <div class="line-2 rte" data-cc-animate data-cc-animate-delay="0.2s">
                  {{ collection.description }}
                </div>
              {% endif %}
            {% else %}
              <h1 class="line-1 feature-header no-margin" data-cc-animate>{{ collection.title }}</h1>
            {% endif %}
          {% endif %}
        </div>
      </div>

    {% if section.settings.show_collection_image and collection.featured_image %}
          </div>
        </div>
      </div>
    {% endif %}

    {%- comment -%} 系列页标签 {%- endcomment -%}
    <!-- 系列页标签 -->
    {% for block in section.blocks %}
      {% case block.type %}
        {% when "collection_text_classify" %}
        {% assign has_tabs = false %}
        {% for temp_collection in block.settings.selected_collections %}
          {% if temp_collection.title == collection.title %}
            {% assign has_tabs = true %}
          {% endif %}
        {% endfor %}
       
        {% if has_tabs %}
          <div class="collection-text-classify">
            <div class="ql-tabs-list">
              {% if block.settings.tab1 != blank or block.settings.tab1 != '' %}
                <a href="{{block.settings.tab1_url}}">{{block.settings.tab1}}</a>
              {% endif %}
              {% if block.settings.tab2 != blank or block.settings.tab2 != '' %}
                <a href="{{block.settings.tab2_url}}">{{block.settings.tab2}}</a>
              {% endif %}
              {% if block.settings.tab3 != blank or block.settings.tab3 != '' %}
                <a href="{{block.settings.tab3_url}}">{{block.settings.tab3}}</a>
              {% endif %}
              {% if block.settings.tab4 != blank or block.settings.tab4 != '' %}
                <a href="{{block.settings.tab4_url}}">{{block.settings.tab4}}</a>
              {% endif %}
              {% if block.settings.tab5 != blank or block.settings.tab5 != '' %}
                <a href="{{block.settings.tab5_url}}">{{block.settings.tab5}}</a>
              {% endif %}
              {% if block.settings.tab6 != blank or block.settings.tab6 != '' %}
                <a href="{{block.settings.tab6_url}}">{{block.settings.tab6}}</a>
              {% endif %}
              {% if block.settings.tab7 != blank or block.settings.tab7 != '' %}
                <a href="{{block.settings.tab7_url}}">{{block.settings.tab7}}</a>
              {% endif %}
              {% if block.settings.tab8 != blank or block.settings.tab8 != '' %}
                <a href="{{block.settings.tab8_url}}">{{block.settings.tab8}}</a>
              {% endif %}
            </div>
          </div>
        {% endif %}
          
      {% endcase %}
    {% endfor %}

    {% if has_description %}
    <!-- 系列页介绍 -->
    {% assign block_width = 100 | divided_by: section.settings.grid %}
    {% style %}
        @media (min-width: 992px){
            #collection_list_carousel .swiper-slide{
                width: {{block_width}}%;
                padding-left: 1rem;
                padding-right: 0;
            }
        }
    {% endstyle %}
    <div class="swiper-container" data-products-in-view="{{ section.settings.grid }}">
        <div class="carousel-box wide-container product-list swiper-wrapper" style="left: 0px;">
            {% assign blocks_num = -1 %}
            {% for block in section.blocks %}
            {% case block.type %}
              {% when "ql_advertising" %}
                {% assign blocks_num = blocks_num | plus: 1 %}
                {% assign img_url = block.settings.ql_image | img_url %}
                <div class="product-block cc-product-block column quarter cc-initialized swiper-slide" {{ block.shopify_attributes }} data-loop-index="{{ forloop.index }}"
                data-cc-animate="cc-fade-in-up" data-cc-animate-delay="{{ forloop.index | times: 0.1 }}s">
                    <div class="product-block__inner">
                        <a href="{{block.settings.ql_url}}">
                            <div class="image" style="padding-top: {{ 1 | divided_by:block.settings.ql_image.aspect_ratio | times: 100 }}%">
                                <div class="image-inner"> 
                                    <div class="image__first">
                                        {% render 'responsive-image', image: block.settings.ql_image %}
                                    </div>
                                </div>
                            </div>
                            
                            <div class="text-area">
                                <div class="heading">
