{% if has_description %}
<script>
    var collects_num = {{ section.settings.grid }};
    var swiper_container = document.querySelector('#collection_list_carousel .swiper-container');
    var carousel_box = document.querySelector('#collection_list_carousel .carousel-box');
    var carousel_box_width = carousel_box.offsetWidth;
    var carousel_slide = document.querySelectorAll('#collection_list_carousel .swiper-slide');
    var carousel_slide_width = carousel_slide[0].offsetWidth;
    var slide_num = carousel_slide.length;
    var carousel_prev_btn = document.querySelector('#collection_list_carousel .swiper-button-prev');
    var carousel_next_btn = document.querySelector('#collection_list_carousel .swiper-button-next');
    var pic_index = 0;
    var swiper_focus = document.querySelector('#collection_list_carousel .swiper-focus');
    // pc端轮播图
    if(slide_num <= collects_num){
        carousel_prev_btn.style.display = 'none';
        carousel_next_btn.style.display = 'none';
    }else{
        carousel_prev_btn.onclick = function(){
            let this_left_width = parseFloat(carousel_box.style.left);
            // console.log("this_left_width", this_left_width)
            this_left_width = this_left_width + carousel_slide_width;
            let max_width = -(slide_num - collects_num) * carousel_slide_width;
            // console.log("this_left_width",this_left_width,"max_width",max_width)
            if(this_left_width > 0){
                carousel_box.style.left = '0px';
                pic_index = 0;
                // carousel_box.style.left = max_width + 'px';
                // pic_index = slide_num - 1;
            }else{
                carousel_box.style.left = this_left_width + 'px';
                pic_index--;
            }
            if(swiper_focus.querySelector('.current')) swiper_focus.querySelector('.current').classList.remove('current');
            swiper_focus.children[pic_index].classList.add('current');
        }
        carousel_next_btn.onclick = function(){
            let this_left_width = parseFloat(carousel_box.style.left);
            let max_width = -(slide_num - collects_num) * carousel_slide_width;
            this_left_width = this_left_width - carousel_slide_width;
            if(this_left_width < max_width){
                carousel_box.style.left = max_width + 'px';
                pic_index = slide_num - 1;
                // carousel_box.style.left = '0px';
                // pic_index = 0;
            }else{
                carousel_box.style.left = this_left_width + 'px';
                pic_index++;
            }
            if(swiper_focus.querySelector('.current')) swiper_focus.querySelector('.current').classList.remove('current');
            swiper_focus.children[pic_index].classList.add('current');
        }
    }

    // 移动端touch事件
      let startPoint = {}; 
      let translateX = 0;
      let startTranslateX = 0;
      let now = 0;
      let wrapW = swiper_container.clientWidth * 0.715;
      const range = .3 * wrapW;
      let isFirst = true;
      let isMove = true;
    //   carousel_box.innerHTML += carousel_box.innerHTML;
 
      // 当手指触摸屏幕时触发
      swiper_container.addEventListener("touchstart", (e) => {
        let touch = e.changedTouches[0];
        carousel_box.style.transition = "none";
        startPoint = {
          x: touch.pageX,
          y: touch.pageY
        };
        if (now == 0) {
          now = 0;
        } else if (now == slide_num - 1) {
          now = slide_num - 1;
        }
        translateX = -now * wrapW;
        carousel_box.style.transform = `translateX(${translateX}px)`;
        startTranslateX = translateX;
        isFirst = true;
        isMove = true;
      });
      
      // 当手指在屏幕上滑动时连续地触发
      swiper_container.addEventListener("touchmove", (e) => {
        let touch = e.changedTouches[0];
        let nowPoint = {
          x: touch.pageX,
          y: touch.pageY
        };
        let dis = {
          x: nowPoint.x - startPoint.x,
          y: nowPoint.y - startPoint.y
        };
        // 在安卓下，手指按下时，如果触摸面积比较大，容易误触 touchmove
        if (isFirst) {
          if (Math.abs(dis.x) - Math.abs(dis.y) > 5) {
            // 左右滑动, 阻止滚动条不进行上下滑动，而让幻灯片滑动
            isMove = true;
            isFirst = false;
          } else if (Math.abs(dis.y) - Math.abs(dis.x) > 5) {
            // 上下滑动, 不阻止滚动条滑动，禁止幻灯片滑动
            isMove = false;
            isFirst = false;
          }
        }
        if (isMove) {
          e.preventDefault();
          if (!isFirst) {
            translateX = startTranslateX + dis.x;
            carousel_box.style.transform = `translateX(${translateX}px)`;
          }
        }
      });
      
      // 当手指从屏幕上离开时触发
      swiper_container.addEventListener("touchend", (e) => {
        let touch = e.changedTouches[0];
        let nowPoint = {
          x: touch.pageX,
          y: touch.pageY
        };
        let dis = {
          x: nowPoint.x - startPoint.x,
          y: nowPoint.y - startPoint.y
        };
        
        if (Math.abs(dis.x) > range && isMove) {
          now -= dis.x / Math.abs(dis.x);
        }
 
        // navs.forEach(item => {
        //   item.classList.remove("active")
        // });
        carousel_box.style.transition = ".3s";
        let real_translateX = 0;
        if (now <= 0) {
          now = 0;
          real_translateX = 0;
        } else if (now >= slide_num - 1) {
          now = slide_num - 1;
          real_translateX = -(now - 0.285) * wrapW;
        }else{
          real_translateX = -now * wrapW;
        }
        // console.log("now",now);
        // console.log("实际右侧边界", now );

        translateX = -now * wrapW;
        carousel_box.style.transform = `translateX(${real_translateX}px)`;

        // navs[now % navs.length].classList.add("active");
      });
  
</script>
{% endif %}


