      if(url.indexOf('variant') != -1){
        url = url.split('?variant')[0];
        url = url + '?variant=' + m_current_variant.id;
      }

      url = url.replace('&default=M', '');
      
      $('[ql-swatch-type]').each(function(i){
        // $(this).find('option[selected]')
        $(this).val(new_options[i]);
      });
      $('.product-purchase-form .original-selector').val(m_current_variant.id);

      window.history.replaceState({
          path: url
        }, '', url);

    }
    const navHeight = $('#site-control .site-control__inner').outerHeight();
    
    $(document).on('scroll', function(){
      $('#ql-selected-variant-info.site-control').css('top', navHeight + 'px');
      var ProductAreaH = $('#page-content .product-area').outerHeight();
      var scrollY = $(this).scrollTop();
      const $selectedEl = $('#ql-selected-variant-info.site-control');
      if(scrollY >= (ProductAreaH)){
        $selectedEl.css('height', 'auto');
      }else{
        $selectedEl.css('height', '0px');
      }
      // console.log("offsetY = " + scrollY,ProductAreaH,  ProductAreaH - descriptElH);
    })
  });
  
