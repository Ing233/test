var _ql_arr_discount_metafield = {{arr_discount_metafield | json}};
if ( typeof(_ql_current_variantid) == 'undefined' ) {
     var _ql_current_variantid =   {{ current_variant.id }};  
} else {
      _ql_current_variantid =   {{ current_variant.id }};
}

var _arr_tending_metafield = {{ arr_tending_metafield | json}};

var _ql_tending_obj = {};
{% for this_m in arr_tending_metafield %}

  {% if this_m.handle != '' %}
    if(_ql_tending_obj["{{this_m.handle}}"]){
      let t_arr = _ql_tending_obj["{{this_m.handle}}"];
      t_arr.push({{this_m | json}});
      _ql_tending_obj["{{this_m.handle}}"] = t_arr;
      
    }else{
      _ql_tending_obj["{{this_m.handle}}"] = [{{this_m | json}}];
    }
  {% endif %}
 
{% endfor %}

console.log('_ql_tending_obj--', _ql_tending_obj);
// 榜单参数
let p_url = new URL(window.location.href);
let ql_tending = p_url.searchParams.get('tending');

</script>


