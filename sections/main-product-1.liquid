    {%- assign arr_hb_all_variants = arr_hb_all_variants | concat: this_product.variants -%}


    {%- comment -%} ootd  {%- endcomment -%}
    {% assign arr_color = this_product.options_by_name['Color'].values %}
    {% for this_tag in this_product.tags %}
      {% assign temp_ootd_arr = this_tag | split: 'OOTD_' %}
      {% if temp_ootd_arr.size == 1 %} {% continue %}{% endif %}

      {% assign temp_ootd = this_tag | replace_first: 'OOTD_', '' | split: '_' %}
      {% if arr_color contains temp_ootd[0] %}
        {% assign t = this_tag | replace_first: 'OOTD_', '' %}
        {% assign tt = this_product.handle | append: '_' | append: t | split: ',_' %}
        {% assign ql_ootd_arr = ql_ootd_arr | concat: tt %}
      {% endif %}
