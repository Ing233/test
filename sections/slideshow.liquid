          {
            "type": "text",
            "id": "button_label",
            "label": "Button"
          },
          {
            "type": "color",
            "id": "button_color",
            "label": "Button color (PC)"
           },
           {
             "type": "color",
             "id": "button_bg",
             "label": "Button background color (PC)"
            },
          {
            "type": "color",
            "id": "mb_button_color",
            "label": "Button color (MB)"
            },
            {
              "type": "color",
              "id": "mb_button_bg",
              "label": "Button background color (MB)"
            },
          {
